{
  "name": "lab3-demo",
  "version": "0.4.0",
  "description": "lab 3 demo",
  "main": "index.js",
  "bin":{
    "dice": "./src/cli/dice-cli.js"
  },
  "scripts": {
    "test": "node --test test/**/*.test.js",
    "test:watch": "node --test --watch test/**/*.test.js",
    "lint": "eslint src/**/*.js test/**/*.js",
    "lint:fix": "eslint src/**/*.js test/**/*.js --fix",
    "format": "prettier --write \"src/**/*.js\" \"test/**/*.js\"",
    "format:check": "prettier --check \"src/**/*.js\" \"test/**/*.js\"",
    "docs": "jsdoc -c jsdoc.json",
    "docs:serve": "npx http-server ./docs/generated -p 8080",
    "quality": "node check-quality.js",
    "cli": "node src/cli/dice-cli.js"
    },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tschnittger2235/lab3-demo.git"
  },
  "author": "Teague Schnittger",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tschnittger2235/lab3-demo/issues"
  },
  "homepage": "https://github.com/tschnittger2235/lab3-demo#readme",
  "devDependencies": {
    "eslint": "^9.35.0",
    "jsdoc": "^4.0.4",
    "prettier": "^3.6.2"
  },
  "type": "module"
}
